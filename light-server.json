{
  "serve": "./public/",
  "servePrefix": "",
  "serveRedirect": false,
  "port": 8080,
  "bind": "localhost",
  "watchexps": [
    "**.js # npm run build",
    "public/*.css # # reloadcss",
    "public/*.html # # reload"
  ],
  "interval": 500,
  "delay": 0,
  "proxy": "http://localhost:9999",
  "proxypaths": ["/api"],
  "noReload": false,
  "quiet": false,
  "open": true,
  "http2": false,
  "historyindex": "/index.html"
}
